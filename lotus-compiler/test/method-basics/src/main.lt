class Foo {
    value: string,
    number: float

    print() {
        @log(this.value);
    }

    set(value: string) {
        this.value = value;
    }

    get() -> float {
        return this.number;
    }
}

export fn main() {
    let foo = Foo {
        value: "hello",
        number: 1.3f
    };

    foo.print();
    foo.set("world");
    foo.print();

    @log(foo.number);
    foo.number = 1.9f;
    @log(foo.number);
}