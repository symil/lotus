export fn main() {
    let base = @alloc(8)[-1] >> 2;
    @trigger_garbage_collection();

    for i in 0..10 {
        let p = @alloc(8);
        @log((p[-1] >> 2) - base);
        @trigger_garbage_collection();
    }

    // Make sure the pointer version is not 0 or a low value
    // If that was the case it would mean the pointer version was not properly randomized
    @log(base > 100);
}