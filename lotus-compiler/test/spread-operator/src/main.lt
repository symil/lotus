class Target {
    a: int,
    b: float,
    c: string
}

class Foo {
    a: int,
    b: int
}

class Bar {
    b: float,
    d: string
}

class WithPrivate1 {
    _a: int,
    a: int
}

class WithPrivate2 {
    _a: int,
    a: int
}

export fn main() {
    let foo = Foo {
        a: 1,
        b: 2,
    };
    let bar = Bar {
        b: 1.2,
        d: "hello"
    };
    let target = Target {
        ..foo,
        ..bar,
        c: "world"
    };
    let target2 = Target {
        ..target,
        a: 8
    };
    let p1 = WithPrivate1 {
        _a: 5,
        a: 6
    };
    let p2 = WithPrivate2 {
        ..p1
    };

    @dbg(foo);
    @dbg(bar);
    @dbg(target);
    @dbg(target2);
    @dbg(p1);
    @dbg(p2);
}