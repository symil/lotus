event MyEvent {

}

class Foo {
    a: int
}

class A {
    @MyEvent {
        yield Foo { a: 8 };
        
        @log(__output.yielded.len());
        @log(__output.yielded[0] is Foo(foo));
        @log(foo.a);
    }
}

export fn main() {
    let array : Object[] = [A];
    let my_event = MyEvent;

    my_event.emit(array);
}