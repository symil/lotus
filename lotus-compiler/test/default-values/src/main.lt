class Foo {
    a: int = 7,
    b: string = some_string(),
    c: float = 0,
    d: Bar = Bar { a: 8 },
    e: Bar = Bar,
    f: Bar? = none
}

class Bar {
    a: int = 15,
    b: float = 1.2f
}

fn some_string() -> string {
    "the string"
}

export fn main() {
    let foo = Foo {
        a: 6
    };

    @log(foo.a);
    @log(foo.b);
    @log(foo.c);
    @log(foo.d.a);
    @log(foo.d.b);
    @log(foo.e.a);
    @log(foo.f as int);

    let foo2 = Foo {};

    @log((foo.d as int) != (foo2.d as int));
}