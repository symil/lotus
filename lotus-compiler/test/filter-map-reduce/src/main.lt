export fn main() {
    let a = [1, 2, 3, 4, 5].filter(is_odd);
    let b = a.map(int_to_string);
    let c = a.reduce(0, add);

    @dbg(a);
    @dbg(b);
    @dbg(c);
}

fn int_to_string(n: int) -> string {
    "number: " + n.to_string()
}

fn is_odd(n: int) -> bool {
    n % 2 == 1
}

fn add(a: int, b: int) -> int {
    a + b
}