export interface Add {
    add(This) -> This;
}

export interface Sub {
    sub(This) -> This;
}

export interface Mul {
    mul(This) -> This;
}

export interface Div {
    div(This) -> This;
}

export interface Mod {
    mod(This) -> This;
}

export interface Shl {
    shl(int) -> This;
}

export interface Shr {
    shr(int) -> This;
}

export interface And {
    and(This) -> This;
}

export interface Or {
    or(This) -> This;
}

export interface Xor {
    xor(This) -> This;
}

export interface Ge {
    ge(This) -> bool;
}

export interface Gt {
    gt(This) -> bool;
}

export interface Le {
    le(This) -> bool;
}

export interface Lt {
    lt(This) -> bool;
}

export interface Plus {
    plus() -> This;
}

export interface Minus {
    minus() -> This;
}

export interface Not {
    not() -> This;
}

export interface ToBool {
    to_bool() -> bool;
}

export interface GetAtIndex {
    type Item;

    get_at(int) -> Item;
}

export interface SetAtIndex {
    type Item;

    set_at(int, Item);
}

export interface Iterable {
    type Item;
    
    get_iterable_len() -> int;
    get_iterable_ptr() -> Pointer<Item>;
}

export interface Unwrap {
    type Item;

    unwrap() -> Item;
}

export interface ToFloat {
    to_float() -> float;
}

export interface Tuple {
    type First;
    type Second;

    first() -> First;
    second() -> Second;
}

export interface Object {
    static __is(Object) -> bool;
}

sys interface Builtin {
    static default() -> This;
    static type_name() -> string;
    static __none() -> This;
    __is_none() -> bool;
    __hash() -> int;

    to_string() -> string;
    to_debug_string(set: Set<ptr>) -> string;
    eq(This) -> bool;
    ne(This) -> bool;
    static __retain(value: This);
    __serialize(buffer: Buffer);
    static __deserialize(buffer: Buffer) -> This?;
}