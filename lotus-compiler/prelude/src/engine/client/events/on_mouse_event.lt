export event OnMouseEvent {
    client: Client,
    user: #USER_TYPE,
    local_data: #LOCAL_DATA_TYPE,
    action: MouseAction,
    button: MouseButton,
    x: float,
    y: float
}

export enum MouseAction {
    Move,
    Down,
    Up,
}

export enum MouseButton {
    Left,
    Middle,
    Right
}