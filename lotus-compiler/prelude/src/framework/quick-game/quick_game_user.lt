export class QuickGameUser {
    name: string = "",
    messages: QuickGameMessage[] = [],
    is_searching_for_game: bool = false,
    selected_game_items: Map<GameItemCategory, Object[]> = Map::new(),
    game: bool, // TODO

    is_logged() -> bool {
        !self.name.is_empty()
    }

    show_error(message: string) {
        self.messages.push(QuickGameMessage {
            content: message,
            kind: MessageKind::Error
        });
    }
}