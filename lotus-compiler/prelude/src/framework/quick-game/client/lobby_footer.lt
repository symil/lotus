pub view LobbyFooter {
    @OnRender {
        self.background_color(Color::green());
        check evt.user.is_logged();
        
        self.layout()
            .outer_margin(.1h)
            .push(MatchmakingButton)
            .aspect_ratio(4)
            .bottom_right_to_left()
            .push(LogoutButton)
            .height(0.5h)
            .aspect_ratio(3)
    }
}