pub class CardFragment {
    graphics: Graphics = Graphics,
    force: float = 1,
    aspect_ratio: float = none,
    direction: Direction = Direction::LeftToRight,
    vertical_margin: DisplaySize = 0,
    horizontal_margin: DisplaySize = 0,
    children: CardFragment[] = [],
}

pub enum Direction {
    LeftToRight,
    TopToBottom
}