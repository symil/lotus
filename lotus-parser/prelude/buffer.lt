pub class Buffer {
    array: int[],
    set: Set<ptr>,

    register<T>(value: T) -> bool {
        this.array.push(value as int);

        if (this.set.add(value as ptr)) {
            return true;
        } else {
            return false;
        }
    }

    push(value: int) {
        this.array.push(value);
    }
}