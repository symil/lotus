export type(T) Option<T : Print> {
    static __default() -> This {
        return T.__none() as This;
    }

    static __none() -> This {
        return T.__none() as This;
    }

    print() {
        if this.__is_none() {
            @log("none");
        } else {
            @log(this as T);
        }
    }
}