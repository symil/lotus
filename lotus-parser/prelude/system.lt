export type(void) system {
    alloc(block_size: int) -> ptr {
        return mem_alloc(block_size);
    }

    free(pointer: ptr) {
        mem_free(pointer);
    }

    log<T>(value: T) {
        let str = value.to_string();

        log_string(str);
    }

    dbg<T>(value: T) {
        let str = value.to_debug_string();

        log_string(str);
    }

    log_empty() {{
        (call $__log_empty)
    }}
}

fn log_string(string) {{
    (call $log_string)
}}