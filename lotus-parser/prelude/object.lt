export class Object {
    autogen static __new() -> This {
        let data = @alloc(#FIELD_COUNT + 1);

        data[0] = #TYPE_ID;

        return data as This;
    }

    autogen static null() -> This {
        return 0 as This;
    }

    autogen static __default() -> This {
        return This.null();
    }

    is_null() -> bool {
        return (this as int) == 0;
    }
}