export class Object {
    autogen static __new() -> This {
        let data = @alloc(#FIELD_COUNT + 1);

        data[0] = #TYPE_ID;

        return data as This;
    }

    autogen static __default() -> This {
        // TODO

        return 0 as This;
    }

    autogen static __none() -> This {{
        (i32.const 0)
    }}

    __is_none() -> bool {{
        (i32.eqz)
    }}
}