class Foo {
    a: int = 8,
    b: string = "hello",
    c: Bar
}

class Bar {
    a: bool
}

export fn main() {
    let foo = Foo {};
    let yes : int? = 8;
    let no : int? = none;
    let p = @alloc(1);

    @log(true);
    @dbg(true);
    @log(-5);
    @dbg(-5);
    @log('5');
    @dbg('5');
    @log(-1.1234f);
    @log(1f / 0f);
    @log(0f / 0f);
    @dbg(-1.1234f);
    @dbg(1f / 0f);
    @dbg(0f / 0f);
    @log("hello");
    @dbg("hello");
    @log(yes);
    @log(no);
    @dbg(yes);
    @dbg(no);
    @log([1, 2, 3]);
    @dbg([1, 2, 3]);
    @log(p);
    @dbg(p);
    log(foo);
    dbg(foo);
}

fn log(obj: Object) {
    @log(obj);
}

fn dbg(obj: Object) {
    @dbg(obj);
}